<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= yield :title %></title>
    <meta name="description" content="">
    <meta name="author" content="">
    <%= stylesheet_link_tag "application", :media => "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
    <%= yield(:head) %>
  </head>
  <body class="<%= @display ? "display" : "normal" %>">
    
    

    <div id="main" role="main">
      
        <div class="<%= @fixed ? "container" : "container-fluid" %>">

          <%= render 'layouts/header_layout' %>          
          <%= render 'layouts/flash_messages' %>                           
          <%= render :partial => 'layouts/edit_layout' if @is_edit_form %>
          <%= render :partial => 'layouts/index_layout', :object => @index, :as => :index if @is_index_table %> 
          <%= render :partial => 'layouts/list_layout', :object => @list, :as => :list if @is_list_table %>  
          <%= yield %>
                    
          <footer>
          </footer>
        
        </div> <!--! end of .container -->
      
     
    </div> <!--! end of #main -->

  </body>
</html>
