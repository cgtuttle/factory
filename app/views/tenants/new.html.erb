<% set_title "Add a company"%>

<% set_width(6, 10) %>

<%= form_for @tenant do |f| %>
	<div class="form-group">
		<%= f.label :name %>
		<%= f.text_field :name, :autofocus => true, :class => "form-control" %>
	</div>

	
  <%= f.submit "Submit", :class => "btn btn-primary" %>
  <%= link_to "Cancel", :back, :class => "btn btn-default" %>
  	
<% end %>
